{
  "name": "public-holidays-generator",
  "version": "1.0.0",
  "description": "Public holiday calendar generator for Austria and Germany",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "generate": "npm run build && node dist/index.js",
    "generate:all": "npm run build && node dist/index.js all",
    "dev": "tsx src/index.ts",
    "watch": "tsc --watch"
  },
  "keywords": ["holidays", "calendar", "ics", "austria", "germany"],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "ics": "^3.7.6"
  }
}
